{
  "index_name": "worldtravel",
  "start_urls": ["https://worldtravel-83q.pages.dev"],
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "lvl0": {
      "selector": ".VPNavBarTitle",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": ".VPContent h1",
    "lvl2": ".VPContent h2",
    "lvl3": ".VPContent h3",
    "lvl4": ".VPContent h4",
    "text": ".VPContent p, .VPContent li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "strip_chars": " .,;:#",
  "custom_settings": {
    "attributesForFaceting": ["lang", "tags"],
    "attributesToRetrieve": [
      "hierarchy",
      "content",
      "anchor",
      "url",
      "url_without_anchor"
    ]
  },
  "conversation_id": ["url"],
  "nb_hits": 10000,
  "js_render": true,
  "js_wait": 1
} 